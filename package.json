    "test": "npm run test:unit",
    "test:unit": "jest test/unit.test.ts"
  },
  "devDependencies": {
    "@rdfjs/types": "^1.0.1",
    "@solid/eslint-config-base": "^0.2.2",
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^26.0.24",
    "@types/n3": "^1.10.1",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.2",
    "@typescript-eslint/parser": "^4.26.2",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "n3": "^1.11.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
