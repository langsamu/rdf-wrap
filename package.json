{
  "name": "rdfjs-wrapper",
  "version": "0.1.1",
  "description": "RDFJS compliant graph wrapping library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/matthieubosquet/rdf-wrap",
  "bugs": "https://github.com/matthieubosquet/rdf-wrap/issues",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "rdfjs",
    "wrapper",
    "rdf",
    "mapping"
  ],
  "scripts": {
